
//Write a program to count the numbers between 1 t 100 which are not divisible by 2,3 & 5 
#include <stdio.h>

int main() {
int count = 0;
for (int i = 1; i <= 100; i++) {
if (i % 2 != 0 && i % 3 != 0 && i % 5 != 0) {
count++;
}
}
printf("The number of integers between 1 and 100 that are not divisible by 2, 3, or 5 is %d.\n", count);
return 0;
}